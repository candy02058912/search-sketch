{
  "name": "search-sketch",
  "description": "Search in sketch with multilang support",
  "version": "0.2.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "search-sketch",
    "manifest": "src/manifest.json",
    "main": "search-sketch.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "fuse.js": "^5.1.0",
    "sketch-module-web-view": "^3.1.4"
  },
  "author": "Candy Tsai <candy02058912@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/candy02058912/search-sketch.git"
  }
}
